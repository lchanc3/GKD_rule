  "name":"Duolingo",
  "groups":[
    {
      "key":0,
      "name":"答對，你太棒了！",
      "desc":"點擊 '繼續'",
      "rules":[
        {
          "fastQuery":true,
          "activityIds":"com.duolingo.session.SessionActivity",
          "matches":"[text=\"繼續\"]"
        }
      ]
    },
    {
