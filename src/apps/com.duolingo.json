{
  "id":"com.duolingo",
  "name":"Duolingo",
  "groups":[
    {
      "key":0,
      "name":"答對，你太棒了！",
      "desc":"點擊繼續",
      "rules":[
        {
          "fastQuery":true,
          "activityIds":"com.duolingo.session.SessionActivity",
          "matches":"[text=\"繼續\"]"
        }
      ]
    },
    {
      "key":1,
      "name":"課程結束",
      "desc":"點擊領取經驗",
      "rules":[
        {
          "fastQuery":true,
          "activityIds":"com.duolingo.session.SessionActivity",
          "matches":"[text=\"領取經驗\"]"
        }
      ]
    },
    {
      "key":2,
      "name":"課程結束後廣告(倒數)",
      "desc":"關閉音量",
      "actionMaximum":1,
      "rules":[
        {
          "fastQuery":true,
          "activityIds":"com.duolingo.sessionend.ads.PlusPromoVideoActivity",
          "matches":"[vid=\"audioButton\"]"
        }
      ]
    },
    {
      "key":3,
      "name":"課程結束後廣告",
      "desc":"點擊X 按鈕",
      "rules":[
        {
          "fastQuery":true,
          "activityIds":"com.duolingo.sessionend.ads.PlusPromoVideoActivity",
          "matches":"[vid=\"xButton\"]"
        }
      ]
    },
    {
      "key":4,
      "name":"Super SPAM",
      "desc":"點擊關閉",
      "rules":[
        {
          "fastQuery":true,
          "activityIds":"com.duolingo.plus.purchaseflow.PlusPurchaseFlowActivity",
          "matches":"[vid=\"noThanksButton\"]"
        }
      ]
    }
  ]
}